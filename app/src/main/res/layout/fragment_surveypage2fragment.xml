<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".surveypage2fragment">


    <TextView
        android:id="@+id/textView10"
        android:layout_width="283dp"
        android:layout_height="37dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="89dp"
        android:layout_marginEnd="64dp"
        android:layout_marginBottom="28dp"
        android:text="What are you looking for?"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/marriageB"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/marriageB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="90dp"
        android:layout_marginEnd="91dp"
        android:layout_marginBottom="11dp"
        android:background="@drawable/choicebordershape"
        android:text="Marriage"
        android:textColor="@color/selectedtextcolor"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/relationshipB"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10" />

    <Button
        android:id="@+id/relationshipB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="90dp"
        android:layout_marginEnd="91dp"
        android:layout_marginBottom="11dp"
        android:background="@drawable/choicebordershape"
        android:text="Relationship"
        android:textColor="@color/selectedtextcolor"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/casualB"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/marriageB" />

    <Button
        android:id="@+id/casualB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="90dp"
        android:layout_marginEnd="91dp"
        android:layout_marginBottom="17dp"
        android:background="@drawable/choicebordershape"
        android:text="Casual"
        android:textColor="@color/selectedtextcolor"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/hookUpB"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/relationshipB" />

    <Button
        android:id="@+id/hookUpB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="90dp"
        android:layout_marginEnd="91dp"
        android:layout_marginBottom="49dp"
        android:background="@drawable/choicebordershape"
        android:text="Hook Up"
        android:textColor="@color/selectedtextcolor"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/nextSurveyB2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/casualB" />

    <Button
        android:id="@+id/nextSurveyB2"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="288dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="49dp"
        android:layout_marginBottom="157dp"
        android:background="@drawable/nextbuttonborder_inactive"
        android:text=">"
        android:textColor="@color/white"
        android:textSize="36sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/hookUpB" />
</androidx.constraintlayout.widget.ConstraintLayout>