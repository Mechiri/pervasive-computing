<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EventForm">

    <TextView
        android:id="@+id/textView"
        android:layout_width="275dp"
        android:layout_height="78dp"
        android:layout_marginStart="59dp"
        android:layout_marginTop="66dp"
        android:layout_marginEnd="59dp"
        android:layout_marginBottom="21dp"
        android:gravity="center"
        android:text="Add Event Log"
        android:textSize="36sp"
        app:layout_constraintBottom_toTopOf="@+id/spinnerPartner"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Spinner
        android:id="@+id/spinnerPartner"
        android:layout_width="138dp"
        android:layout_height="40dp"
        android:layout_marginStart="36dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="19dp"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toStartOf="@+id/spinnerEvent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <Spinner
        android:id="@+id/spinnerEvent"
        android:layout_width="138dp"
        android:layout_height="40dp"
        android:layout_marginTop="21dp"
        android:layout_marginEnd="41dp"
        android:layout_marginBottom="19dp"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/spinnerPartner"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <SeekBar
        android:id="@+id/timesBar"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="337dp"
        android:layout_height="47dp"
        android:layout_marginStart="28dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="12dp"
        android:max="100"
        android:progress="3"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <SeekBar
        android:id="@+id/convsBar"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="337dp"
        android:layout_height="47dp"
        android:layout_marginStart="28dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="12dp"
        android:max="100"
        android:progress="3"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="55dp"
        android:layout_marginEnd="55dp"
        android:layout_marginBottom="11dp"
        android:text="Rate partner's conversation quality:"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/convsBar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinnerPartner" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="62dp"
        android:layout_marginEnd="62dp"
        android:layout_marginBottom="10dp"
        android:text="Rate partner's use of quality time:"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/timesBar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/convsBar" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="91dp"
        android:layout_marginEnd="91dp"
        android:layout_marginBottom="14dp"
        android:text="Rate partner's use of gifts:"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/giftsBar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/timesBar" />

    <SeekBar
        android:id="@+id/giftsBar"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="337dp"
        android:layout_height="47dp"
        android:layout_marginStart="36dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="10dp"
        android:max="100"
        android:progress="3"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="288dp"
        android:layout_height="37dp"
        android:layout_marginStart="55dp"
        android:layout_marginEnd="50dp"
        android:layout_marginBottom="8dp"
        android:text="Rate partner's use of service acts:"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/servicesBar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/giftsBar" />

    <SeekBar
        android:id="@+id/servicesBar"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="337dp"
        android:layout_height="47dp"
        android:layout_marginStart="28dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="12dp"
        android:max="100"
        android:progress="3"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginEnd="51dp"
        android:layout_marginBottom="18dp"
        android:text="Rate partner's use of physical touch:"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/touchsBar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/servicesBar" />

    <SeekBar
        android:id="@+id/touchsBar"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="337dp"
        android:layout_height="47dp"
        android:layout_marginStart="28dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="85dp"
        android:max="100"
        android:progress="3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

</androidx.constraintlayout.widget.ConstraintLayout>