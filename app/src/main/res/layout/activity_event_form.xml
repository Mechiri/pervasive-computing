<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EventForm">

    <SeekBar
        android:id="@+id/timesBar"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="337dp"
        android:layout_height="47dp"
        android:layout_marginStart="26dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="20dp"
        android:max="100"
        android:progress="3"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="126dp"
        android:layout_marginBottom="24dp"
        android:text="Add Event Log"
        android:textSize="36sp"
        app:layout_constraintBottom_toTopOf="@+id/spinnerPartner"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <SeekBar
        android:id="@+id/convsBar"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="337dp"
        android:layout_height="47dp"
        android:layout_marginStart="26dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="10dp"
        android:max="100"
        android:progress="3"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginEnd="74dp"
        android:layout_marginBottom="8dp"
        android:text="Rate partner's conversation quality:"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/convsBar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinnerPartner" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginEnd="88dp"
        android:layout_marginBottom="18dp"
        android:text="Rate partner's use of quality time:"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/timesBar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/convsBar" />

    <Spinner
        android:id="@+id/spinnerPartner"
        android:layout_width="147dp"
        android:layout_height="42dp"
        android:layout_marginStart="38dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="52dp"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toStartOf="@+id/spinnerEvent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="38dp"
        android:layout_marginEnd="144dp"
        android:layout_marginBottom="8dp"
        android:text="Rate partner's use of gifts:"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/giftsBar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/timesBar" />

    <SeekBar
        android:id="@+id/giftsBar"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="337dp"
        android:layout_height="47dp"
        android:layout_marginStart="26dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="12dp"
        android:max="100"
        android:progress="3"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="288dp"
        android:layout_height="37dp"
        android:layout_marginStart="36dp"
        android:layout_marginEnd="69dp"
        android:layout_marginBottom="6dp"
        android:text="Rate partner's use of service acts:"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/servicesBar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/giftsBar" />

    <SeekBar
        android:id="@+id/servicesBar"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="337dp"
        android:layout_height="47dp"
        android:layout_marginStart="26dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="3dp"
        android:max="100"
        android:progress="3"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="41dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="60dp"
        android:layout_marginBottom="16dp"
        android:text="Rate partner's use of physical touch:"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/touchsBar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/servicesBar" />

    <SeekBar
        android:id="@+id/touchsBar"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="337dp"
        android:layout_height="47dp"
        android:layout_marginStart="26dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="113dp"
        android:max="100"
        android:progress="3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <Spinner
        android:id="@+id/spinnerEvent"
        android:layout_width="147dp"
        android:layout_height="42dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="46dp"
        android:layout_marginBottom="52dp"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/spinnerPartner"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

</androidx.constraintlayout.widget.ConstraintLayout>