<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".surveyQ6fragment">


    <Button
        android:id="@+id/Q6E"
        android:layout_width="261dp"
        android:layout_height="58dp"
        android:layout_marginStart="75dp"
        android:layout_marginEnd="75dp"
        android:layout_marginBottom="43dp"
        android:background="@drawable/choicebordershape"
        android:text="I like to hold hands with someone"
        android:textColor="@color/selectedtextcolor"
        app:layout_constraintBottom_toTopOf="@+id/Q6surveyB"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Q6B" />

    <Button
        android:id="@+id/Q6B"
        android:layout_width="261dp"
        android:layout_height="58dp"
        android:layout_marginStart="75dp"
        android:layout_marginEnd="75dp"
        android:layout_marginBottom="27dp"
        android:background="@drawable/choicebordershape"
        android:text="I like to go places with someone"
        android:textColor="@color/selectedtextcolor"
        app:layout_constraintBottom_toTopOf="@+id/Q6E"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView23" />

    <Button
        android:id="@+id/Q6surveyB"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="279dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="72dp"
        android:layout_marginBottom="266dp"
        android:background="@drawable/nextbuttonborder_inactive"
        android:text=">"
        android:textColor="@color/white"
        android:textSize="36sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Q6E" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="268dp"
        android:layout_height="55dp"
        android:layout_marginStart="71dp"
        android:layout_marginTop="122dp"
        android:layout_marginEnd="72dp"
        android:layout_marginBottom="42dp"
        android:text="Please pick the option that best reflects your preferences."
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/Q6B"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>